{
    "name": "server",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "export NODE_ENV=development || SET \"NODE_ENV=development\" && node server/index.js",
        "test": "export NODE_ENV=test || SET \"NODE_ENV=test\" && mocha server/**/*.test.js --timeout=45000",
        "test-watch": "nodemon --exec \"npm test -- --timeout=45000\"",
        "server": "nodemon server/index.js",
        "client": "npm run start --prefix client",
        "dev": "concurrently \"npm run server\" \"npm run client\""
    },
