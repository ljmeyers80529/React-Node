        "concurrently": "^3.6.0",
        "cookie-session": "^2.0.0-beta.3",
        "express": "^4.16.3",
        "mongodb": "^3.0.10",
        "mongoose": "^5.1.6",
        "passport": "^0.4.0",
        "passport-google-oauth20": "^1.0.0"
    },
    "devDependencies": {
        "nodemon": "^1.17.5"
    }
}
